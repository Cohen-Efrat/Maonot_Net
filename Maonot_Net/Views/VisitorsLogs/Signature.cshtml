@model Maonot_Net.Models.User

@{
    ViewData["Title"] = "Signature";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Signature</h2>
<div class="row">
    <div class="col-md-4">
        <form asp-controller="VisitorsLogs" asp-action="Signature">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StundetId" class="control-label"></label>
                <input asp-for="StundetId" class="form-control" />

            </div>

            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" />

            </div>


            <div class="form-group">
                <input type="submit" value="חתימה" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

